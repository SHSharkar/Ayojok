{
    "name": "nvisio/ayojok",
    "description": "One Stop Solution for your event",
    "keywords": [
        "ayojok",
        "nvisio"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.0",
        "alaouy/youtube": "^2.2",
        "barryvdh/laravel-dompdf": "^0.8.4",
        "fideloper/proxy": "^4.1",
        "guzzlehttp/guzzle": "^6.3",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "5.8.*",
        "laravel/socialite": "^4.1",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.8",
        "spatie/laravel-analytics": "^3.7",
        "willvincent/feeds": "^1.1",
        "ext-json": "*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "dev-master as 2.6.x-dev",
        "doctrine/dbal": "^2.9",
        "filp/whoops": "^2.4",
        "fzaninotto/faker": "^1.8",
        "mockery/mockery": "^1.2",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.0",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "composer dumpautoload",
            "php artisan cache:clear",
            "php artisan clear-compiled",
            "php artisan config:clear",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:models -N",
            "php artisan optimize:clear",
            "php artisan route:clear",
            "php artisan view:cache",
            "php artisan view:clear"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "composer dumpautoload",
            "php artisan cache:clear",
            "php artisan clear-compiled",
            "php artisan config:clear",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:models -N",
            "php artisan optimize:clear",
            "php artisan route:clear",
            "php artisan view:cache",
            "php artisan view:clear"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Sazzad Hossain Nirjhor",
            "homepage": "https://github.com/Nirjhor3029",
            "role": "Developer"
        },
        {
            "name": "Sazzad Hossain Sharkar",
            "email": "sharkar@akhaura.info",
            "homepage": "https://github.com/SHSharkar",
            "role": "Developer"
        }
    ],
    "homepage": "https://ayojok.com"
}
